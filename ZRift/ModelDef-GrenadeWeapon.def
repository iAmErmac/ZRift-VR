Model ChasmRocketL
{
   Path "models/weapons"   	
   Model 0 "GrenadeWeapon.md2"   
   Skin 0 "GrenadeWeapon.png"
   OFFSET -1 -18 -9	         //X-Left/Right Y-Depth Z-Up/Down
   AngleOffset -90         //angle  
   PitchOffset 0		     //pitch Left Right
   RollOffset 0  	     //roll Up-Down  
   Scale -0.1 0.1 0.1    //Scale (negative value is to mirror left right)

//Idle
   Frame GREX Z 0 "IdleA1"
}

